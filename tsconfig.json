{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/sections/*": [
        "src/sections/*"
      ],
      "@/IndexPage/*": [
        "src/sections/IndexPage/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
